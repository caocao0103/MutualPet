.navigator-hover{
  background: rgba(0, 0, 0, 0.01);
}

.model-bg{
  width:100%;
  height:100%;
  position:fixed;
  top:0;
  left:0;
  background:#000;
  opacity:0.5;
  overflow:hidden;
  z-index:9999999999;
}
.model-content{
  position:fixed;
  top:30%;
  left:96rpx;
  z-index:99999999999;
  width:560rpx;
  height:460rpx;
  overflow:hidden;
  background:#fff;
  border-radius:10rpx;
}
.model-tu{
  width: 490rpx;
  height: 230rpx;
  margin: 40rpx auto;
}
.model-photo{
  width: 100%;
  height: 100%;
}
.model-words{
  font-family: PingFangSC-Regular;
  font-size: 14px;
  color: #666666;
  letter-spacing: 0.17px;
  line-height: 14px;
  margin-left: 36rpx;
}
.model-button{
  width: 100%;
  height: 90rpx;
  position: absolute;
  bottom:0;
  line-height: 90rpx;
  background: #FF6F1D;
  font-size: 15px;
  color: #333333;
  border-radius: 0 0 10rpx 10rpx;
}
.model-button::after{
  border: none;
  border-radius: none;
}


.awards {
  width: 70rpx;
  height: 40rpx;
  border: 2rpx solid #FF6F1D;
  border-radius: 4rpx;
  font-size: 22rpx;
  color: #FF6F1D;
  letter-spacing: 0.12rpx;
  text-align: center;
  line-height: 40rpx;
  float: right;
  margin-right: 28%;
  margin-top: 24rpx; 
}
.awardss {
  width: 70rpx;
  height: 40rpx;
  border: 2rpx solid #FF6F1D;
  border-radius: 4rpx;
  font-size: 22rpx;
  color: #FF6F1D;
  letter-spacing: 0.12rpx;
  text-align: center;
  line-height: 40rpx;
  display: inline-block;
  margin-left: 10rpx;
}
.title_sixin{
  overflow:hidden;
  display:-webkit-box;
  white-space:normal !important;
  text-overflow:ellipsis;
  word-wrap:break-word;
  -webkit-line-clamp:1;
  -webkit-box-orient:vertical;
  max-width: 240rpx;
  margin-left: 30%;
  float: left;
}

/* 举报弹框 */
.meng{
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.5);
  z-index: 9;
}
.jub_model{
  position: absolute;
  width: 574rpx;
  height: 680rpx;
  top: 300rpx;
  left: 50%;
  transform: translateX(-50%);
  z-index: 99;
  background: #fff;
  border-radius: 10px;
}
.jub_top{
  height: 100rpx;
  padding: 58rpx 60rpx 0 60rpx;
  border-radius: 10px 10px 0 0;
}
.jub_top image{
  width: 48rpx;
  height: 48rpx;
  float: left;
}
.jub_top view{
  font-size: 30rpx;
  color: #444444;
  margin-left: 70rpx;
  font-weight: bold;
}
.jub_center{
  font-size: 28rpx;
  color: #444444;
  height: 74rpx;
  padding: 0 60rpx;
}
checkbox{
  width: 36rpx;
  height: 36rpx;
  float: right;
}
checkbox .wx-checkbox-input {
  width: 36rpx;
  height: 36rpx;
}
checkbox .wx-checkbox-input.wx-checkbox-input-checked {
  border: 1px solid #FF6F1D;
}
checkbox .wx-checkbox-input.wx-checkbox-input-checked::before {
  width: 28rpx;
  height: 28rpx;
  line-height: 28rpx;
  text-align: center;
  font-size: 22rpx;
  color: #FF6F1D;
  background: transparent;
  transform: translate(-50%, -50%) scale(1);
  -webkit-transform: translate(-50%, -50%) scale(1);
}
.kefus{
  color: #2D83DB;
  background: #fff;
}
.jub_tijao{
  border-radius: 0 0 10px 10px;
  font-size: 28rpx;
  color: #FF6F1D;
  height: 86rpx;
  background: #fff;
  line-height: 86rpx;
  border-top: 1px solid #eee; 
  width: 100%;
  padding: 0;
}


