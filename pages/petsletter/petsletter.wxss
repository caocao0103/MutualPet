/* 顶部状态栏 */
.titles{
  width: 100%;
  height: 44px;
  text-align: center;
  line-height: 44px;
  font-size: 16px;
  background: #fff;
  position: fixed;
  top: 0;
  border-bottom: 1px solid #eee;
  z-index: 99999999;
}
.tabss{
  z-index: 999999999;
  position: fixed;
  left: 8px;
  top:6px;
  width: 87px;
  height: 31px;
  border-radius: 16px;
  align-items: center;
  display: -webkit-flex;
  border:1px solid rgba(0,0,0,0.08);
}
.fanhui{
  height: 19px;
  width:43px;
