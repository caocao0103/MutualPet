/* packageD/pages/petMyRelease/petMyRelease.wxss */
.myAdopt-petsLi--btn {
  width:484rpx;
  height:88rpx;
  background:rgba(255,111,29,1);
  box-shadow:0px 4rpx 16rpx 0px rgba(255,100,0,0.3);
  border-radius:63rpx;
  position: fixed;
  bottom: 30rpx;
  left: 50%;
  transform: translateX(-50%);
  color: #FFFFFF;
  font-size: 30rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.myAdopt-pet__empty {
  position: fixed;
  width: 100%;
  height: 100%;
  left: 0;
  top: 0;
  background: #fff;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.myAdopt-pet__empty--img {
  width: 400rpx;
  height: 276rpx;
}
.myAdopt-pet__empty--desc {
  color: #AAAAAA;
  font-size: 26rpx;
  margin-top:30rpx;
}